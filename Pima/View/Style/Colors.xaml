<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Pima.View.Style">

    <SolidColorBrush x:Key="DarkBaseBrush" Color="#4600a8" />
    <Color x:Key="DarkBaseColor">#4600a8</Color>

    <SolidColorBrush x:Key="MediumBaseBrush" Color="#512da8" />
    <Color x:Key="MediumBaseColor">#512da8</Color>

    <SolidColorBrush x:Key="LightBaseBrush" Color="#FFBE9CFF" />
    <Color x:Key="LightBaseColor">#8559da</Color>

    <SolidColorBrush x:Key="AccentBrush" Color="#ADFF2F" />
    <Color x:Key="AccentColor">#ADFF2F</Color>

    <SolidColorBrush x:Key="DarkBrush" Color="Black" />
    <Color x:Key="DarkColor">Black</Color>

    <SolidColorBrush x:Key="LightBrush" Color="white" />
    <Color x:Key="LightColor">white</Color>

</ResourceDictionary>